{
  "name": "notes_demo",
  "version": "1.0.0",
  "description": "simple React & Redux demo application for github account",
  "main": "index.js",
  "scripts": {
    "build:development": "webpack --config webpack.config.dev.js",
    "build:production": "webpack --config webpack.config.prod.js",
